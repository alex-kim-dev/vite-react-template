{
  "name": "vite-react-template",
  "version": "1.0.0-alpha.0",
  "private": true,
  "description": "Vite template for React Typescript projects",
  "license": "MIT",
  "repository": "git@github.com:alex-kim-dev/vite-react-template.git",
  "author": "Alex Kim (https://github.com/alex-kim-dev)",
  "homepage": "https://github.com/alex-kim-dev/vite-react-template#readme",
  "bugs": "https://github.com/alex-kim-dev/vite-react-template/issues",
  "scripts": {
    "build": "vite build",
    "format": "prettier --check --ignore-path .gitignore \"**/*.{html,json,yml,md}\"",
    "lint:styles": "stylelint --cache --ignore-path .gitignore \"**/*.css\"",
    "lint:ts": "eslint --cache --ignore-path .gitignore --ext .ts,.tsx .",
    "prepare": "husky install",
    "serve": "vite preview",
    "start": "vite",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.0",
    "@commitlint/config-conventional": "17.4.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.2.5",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "@vitejs/plugin-react": "3.0.1",
    "eslint": "8.31.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.2.0",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-testing-library": "5.9.1",
    "husky": "8.0.3",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1",
    "prettier-plugin-package": "1.3.0",
    "stylelint": "14.16.1",
    "stylelint-config-idiomatic-order": "9.0.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-standard": "29.0.0",
    "stylelint-order": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "ts-jest": "29.0.3",
    "tsc-files": "1.1.3",
    "typescript": "4.9.4",
    "vite": "4.0.4",
    "vite-plugin-html": "3.2.0",
    "vite-plugin-svgr": "2.4.0",
    "vite-tsconfig-paths": "4.0.3"
  }
}
