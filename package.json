{
  "name": "vite-react-template",
  "version": "1.0.0-alpha.0",
  "private": true,
  "description": "Vite template for React Typescript projects",
  "license": "MIT",
  "repository": "git@github.com:alex-kim-dev/vite-react-template.git",
  "author": "Alex Kim (https://github.com/alex-kim-dev)",
  "homepage": "https://github.com/alex-kim-dev/vite-react-template#readme",
  "bugs": "https://github.com/alex-kim-dev/vite-react-template/issues",
  "scripts": {
    "build": "vite build",
    "format": "prettier --check \"**/*.{html,json,yml,md}\"",
    "lint:styles": "stylelint --cache --ignore-path .gitignore \"**/*.css\"",
    "lint:ts": "eslint --cache --ignore-path .gitignore --ext .ts,.tsx .",
    "prepare": "husky install",
    "serve": "vite preview",
    "start": "vite",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@testing-library/jest-dom": "6.1.3",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "@vitejs/plugin-react-swc": "3.3.2",
    "@vitest/coverage-v8": "0.34.4",
    "eslint": "8.49.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-testing-library": "6.0.1",
    "eslint-plugin-vitest": "0.3.1",
    "happy-dom": "12.0.1",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-package": "1.3.1",
    "stylelint": "15.10.3",
    "stylelint-config-idiomatic-order": "9.0.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-order": "6.0.3",
    "stylelint-prettier": "4.0.2",
    "tsc-files": "1.1.4",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-plugin-html": "3.2.0",
    "vite-plugin-svgr": "3.2.0",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.4"
  },
  "overrides": {
    "prettier-plugin-package@1.3.1": {
      "prettier": "^3.0.0"
    },
    "stylelint-order@5.0.0": {
      "stylelint": "^15.0.0"
    }
  }
}
