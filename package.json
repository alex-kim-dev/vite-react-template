{
  "name": "vite-react-template",
  "version": "1.0.0-alpha.0",
  "private": true,
  "description": "Vite template for React Typescript projects",
  "homepage": "https://github.com/alex-kim-dev/vite-react-template#readme",
  "bugs": "https://github.com/alex-kim-dev/vite-react-template/issues",
  "repository": "git@github.com:alex-kim-dev/vite-react-template.git",
  "license": "MIT",
  "author": "Alex Kim (https://github.com/alex-kim-dev)",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "format": "prettier --check \"**/*.{html,json,yml,md}\"",
    "lint:code": "eslint --cache --ignore-path .gitignore --ext .ts,.tsx .",
    "lint:styles": "stylelint --cache --ignore-path .gitignore \"**/*.css\"",
    "prepare": "husky",
    "serve": "vite preview",
    "start": "vite",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@testing-library/jest-dom": "6.4.6",
    "@testing-library/react": "16.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "@vitejs/plugin-react-swc": "3.7.0",
    "@vitest/coverage-v8": "1.6.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest-dom": "5.4.0",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.3",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-testing-library": "6.2.2",
    "eslint-plugin-vitest": "0.4.1",
    "happy-dom": "14.12.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "prettier": "3.3.2",
    "prettier-plugin-packagejson": "2.5.0",
    "stylelint": "16.6.1",
    "stylelint-config-recess-order": "5.0.1",
    "stylelint-config-standard": "36.0.0",
    "stylelint-order": "6.0.4",
    "stylelint-prettier": "5.0.0",
    "tsc-files": "1.1.4",
    "typescript": "5.5.2",
    "vite": "5.3.1",
    "vite-plugin-svgr": "4.2.0",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.0"
  }
}
