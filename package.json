{
  "name": "vite-react-template",
  "version": "1.0.0-alpha.0",
  "private": true,
  "description": "Vite template for React Typescript projects",
  "homepage": "https://github.com/alex-kim-dev/vite-react-template#readme",
  "bugs": "https://github.com/alex-kim-dev/vite-react-template/issues",
  "repository": "git@github.com:alex-kim-dev/vite-react-template.git",
  "license": "MIT",
  "author": "Alex Kim (https://github.com/alex-kim-dev)",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "format": "prettier --list-different .",
    "format:fix": "pnpm run format --write",
    "lint": "eslint . --report-unused-disable-directives",
    "lint:fix": "pnpm run lint --fix",
    "prepare": "husky",
    "preview": "vite preview",
    "serve": "vite preview",
    "stylelint": "stylelint --ignore-path .gitignore \"**/*.css\" --allow-empty-input",
    "stylelint:fix": "pnpm run stylelint --fix strict",
    "test": "vitest",
    "test:ci": "vitest run --passWithNoTests",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "clsx": "~2.1.1",
    "react": "~19.1.0",
    "react-dom": "~19.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "~19.8.1",
    "@commitlint/config-conventional": "~19.8.1",
    "@eslint/js": "~9.30.1",
    "@testing-library/dom": "~10.4.1",
    "@testing-library/jest-dom": "~6.7.0",
    "@testing-library/react": "~16.3.0",
    "@testing-library/user-event": "~14.6.1",
    "@types/react": "~19.1.8",
    "@types/react-dom": "~19.1.6",
    "@vitejs/plugin-react-swc": "~3.10.2",
    "@vitest/eslint-plugin": "~1.3.4",
    "eslint": "~9.30.1",
    "eslint-config-prettier": "~10.1.5",
    "eslint-import-resolver-typescript": "~4.4.4",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-jsx-a11y": "~6.10.2",
    "eslint-plugin-perfectionist": "~4.15.0",
    "eslint-plugin-react": "~7.37.5",
    "eslint-plugin-react-hooks": "~5.2.0",
    "eslint-plugin-react-refresh": "~0.4.20",
    "globals": "~16.3.0",
    "happy-dom": "~18.0.1",
    "husky": "~9.1.7",
    "lint-staged": "~16.1.2",
    "prettier": "~3.6.2",
    "prettier-plugin-packagejson": "~2.5.18",
    "stylelint": "~16.21.1",
    "stylelint-config-recess-order": "~7.1.0",
    "stylelint-config-standard": "~38.0.0",
    "stylelint-order": "~7.0.0",
    "stylelint-use-logical-spec": "~5.0.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "~8.35.1",
    "vite": "~7.0.2",
    "vite-plugin-mkcert": "~1.17.8",
    "vite-plugin-svgr": "~4.3.0",
    "vite-tsconfig-paths": "~5.1.4",
    "vitest": "~3.2.4"
  }
}
