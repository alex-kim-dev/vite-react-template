{
  "name": "vite-react-template",
  "version": "1.0.0-alpha.0",
  "private": true,
  "description": "Vite template for React Typescript projects",
  "homepage": "https://github.com/alex-kim-dev/vite-react-template#readme",
  "bugs": "https://github.com/alex-kim-dev/vite-react-template/issues",
  "repository": "git@github.com:alex-kim-dev/vite-react-template.git",
  "license": "MIT",
  "author": "Alex Kim (https://github.com/alex-kim-dev)",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "format": "prettier --check . \"!pnpm-lock.yaml\"",
    "lint": "conc pnpm:lint:*",
    "lint:code": "eslint *.js *.ts src",
    "lint:styles": "stylelint --ignore-path .gitignore \"**/*.css\"",
    "prepare": "husky",
    "serve": "vite preview",
    "start": "vite",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@eslint/js": "9.6.0",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "15.0.7",
    "@testing-library/user-event": "14.5.2",
    "@types/eslint__js": "8.42.3",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react-swc": "3.7.0",
    "@vitest/coverage-v8": "1.6.0",
    "concurrently": "8.2.2",
    "eslint": "9.6.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-vitest": "0.5.4",
    "happy-dom": "14.11.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.4",
    "prettier": "3.3.2",
    "prettier-plugin-packagejson": "2.5.0",
    "stylelint": "16.6.1",
    "stylelint-config-recess-order": "5.0.1",
    "stylelint-config-standard": "36.0.1",
    "stylelint-order": "6.0.4",
    "tsc-files": "1.1.4",
    "typescript": "5.5.3",
    "typescript-eslint": "7.16.0",
    "vite": "5.2.11",
    "vite-plugin-svgr": "4.2.0",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.0"
  }
}
