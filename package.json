{
  "name": "vite-react-template",
  "version": "1.0.0-alpha.0",
  "private": true,
  "description": "Vite template for React Typescript projects",
  "license": "MIT",
  "repository": "git@github.com:alex-kim-dev/vite-react-template.git",
  "author": "Alex Kim (https://github.com/alex-kim-dev)",
  "homepage": "https://alex-kim-dev.github.io/vite-react-template#readme",
  "bugs": "https://github.com/alex-kim-dev/vite-react-template/issues",
  "scripts": {
    "build": "vite build",
    "format": "prettier --check --ignore-path .gitignore \"**/*.{html,json,yml,md}\"",
    "lint:styles": "stylelint --cache --ignore-path .gitignore \"**/*.css\"",
    "lint:ts": "eslint --cache --ignore-path .gitignore --ext .ts,.tsx .",
    "prepare": "husky install",
    "serve": "vite preview",
    "start": "vite",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc"
  },
  "dependencies": {
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@testing-library/jest-dom": "5.16.3",
    "@testing-library/react": "13.0.0",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "14.0.4",
    "@types/jest": "27.4.1",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@types/testing-library__jest-dom": "5.14.3",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "@vitejs/plugin-react": "1.3.0",
    "eslint": "8.12.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "16.2.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.3",
    "eslint-plugin-jest-dom": "4.0.1",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-testing-library": "5.2.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "prettier": "2.6.1",
    "prettier-plugin-package": "1.3.0",
    "stylelint": "14.6.1",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "25.0.0",
    "stylelint-order": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "ts-jest": "27.1.4",
    "tsc-files": "1.1.3",
    "typescript": "4.6.3",
    "vite": "2.9.1",
    "vite-plugin-html": "3.2.0",
    "vite-plugin-svgr": "1.1.0",
    "vite-tsconfig-paths": "3.4.1"
  }
}
